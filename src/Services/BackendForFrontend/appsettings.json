{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "MetricOptions": {
    "ServiceName" : "Backend For Frontend",
    "Version" : "1.0",
    "OtlpExportUrl" : "http://otel-collector:4317",
    "AddAspNetCoreMetrics" : true,
    "AddHttpClientMetrics" : true,
    "AddRuntimeMetrics" : true
  }
}
